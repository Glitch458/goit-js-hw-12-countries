<div class='card'>
  <div class='card-data'>
    <h1 class='country-name'>{{name}}</h1>
    <ul class='country-list'>
      <li>CAPITAL: {{capital}}</li>
      <li>POPULATION: {{population}}</li>
      <ul class='lang-list'>LANGUAGES:
        {{#each languages}}
          <li>{{name}}</li>
        {{/each}}
      </ul>
    </ul>
  </div>
  <div class='card-img'>
    <img src='{{flags.svg}}' alt='' width='200px' />
  </div>
</div>